<?xml version="1.0" encoding="UTF-8"?>
<!-- System: SUN FIRE X4270 M3 -->
<!-- ILOM Version: 3.1.2.44 -->
<BIOS>
  <Config>
    <Advanced>
      <Processors>
        <!-- Hyper-threading -->
        <!-- Description: Enabled for Windows XP and Linux (OS optimized for Hyper-Threading Technology) and Disabled for other OS (OS not optimized for Hyper-Threading Technology). When Disabled only one thread per enabled core is enabled. -->
        <!-- Possible Values: "Disabled", "Enabled" -->
        <Hyper-threading>Enabled</Hyper-threading>

        <!-- Execute Disable Bit -->
        <!-- Description: XD can prevent certain classes of malicious buffer overflow attacks when combined with a supporting OS (Windows Server 2003 SP1, Windows XP SP2, SuSE Linux 9.2, RedHat Enterprise 3 Update 3.) -->
        <!-- Possible Values: "Disabled", "Enabled" -->
        <Execute_Disable_Bit>Enabled</Execute_Disable_Bit>

        <!-- Hardware Prefetcher -->
        <!-- Description: Enable the Mid Level Cache (L2) streamer prefetcher. -->
        <!-- Possible Values: "Disabled", "Enabled" -->
        <Hardware_Prefetcher>Enabled</Hardware_Prefetcher>

        <!-- Adjacent Cache Line Prefetch -->
        <!-- Description: Enable the Mid Level Cache (L2) prefetching of adjacent cache lines. -->
        <!-- Possible Values: "Disabled", "Enabled" -->
        <Adjacent_Cache_Line_Prefetch>Enabled</Adjacent_Cache_Line_Prefetch>

        <!-- DCU Streamer Prefetcher -->
        <!-- Description: Enable prefetch of next L1 Data line based upon multiple loads in same cache line. -->
        <!-- Possible Values: "Disabled", "Enabled" -->
        <DCU_Streamer_Prefetcher>Enabled</DCU_Streamer_Prefetcher>

        <!-- DCU IP Prefetcher -->
        <!-- Description: Enable prefetch of next L1 line based upon sequential load history. -->
        <!-- Possible Values: "Disabled", "Enabled" -->
        <DCU_IP_Prefetcher>Enabled</DCU_IP_Prefetcher>

        <!-- Intel Virtualization Technology -->
        <!-- Description: When enabled, a VMM can utilize the additional hardware capabilities provided by Vanderpool Technology -->
        <!-- Possible Values: "Disabled", "Enabled" -->
        <Intel_Virtualization_Technology>Enabled</Intel_Virtualization_Technology>

        <CPU_Power_Management_Configuration>
          <!-- Power Technology -->
          <!-- Description: Enable the power management features. -->
          <!-- Possible Values: "Disable", "Energy Efficient", "Custom" -->
          <Power_Technology>Custom</Power_Technology>

          <!-- Intel SpeedStep -->
          <!-- Description: Enable/Disable Intel SpeedStep -->
          <!-- Possible Values: "Disabled", "Enabled" -->
          <Intel_SpeedStep>Enabled</Intel_SpeedStep>

          <!-- Turbo Mode -->
          <!-- Description: Turbo Mode. -->
          <!-- Possible Values: "Disabled", "Enabled" -->
          <Turbo_Mode>Enabled</Turbo_Mode>

          <!-- CPU C3 Report -->
          <!-- Description: Enable/Disable CPU Core C3 report to OS -->
          <!-- Possible Values: "Disabled", "Enabled" -->
          <CPU_C3_Report>Disabled</CPU_C3_Report>

          <!-- CPU C6 report -->
          <!-- Description: Enable/Disable CPU Core C6 report to OS -->
          <!-- Possible Values: "Disabled", "Enabled" -->
          <CPU_C6_report>Disabled</CPU_C6_report>

          <!-- CPU C7 report -->
          <!-- Description: Enable/Disable CPU Core C7 report to OS -->
          <!-- Possible Values: "Disabled", "Enabled" -->
          <CPU_C7_report>Disabled</CPU_C7_report>

          <!-- Package C-States -->
          <!-- Description: Package C State limit -->
          <!-- Possible Values: "Disabled", "C2", "C6", "C7", "Enabled" -->
          <Package_C-States>Disabled</Package_C-States>

          <!-- Energy Performance -->
          <!-- Description: Optimize between performance and power savings. Windows 2008 and later OSes overrides this value according to its power plan. -->
          <!-- Possible Values: "Performance", "Balanced Performance", "Balanced Energy", "Energy Efficient" -->
          <Energy_Performance>Performance</Energy_Performance>

          <!-- Uncore Frequency Scaling -->
          <!-- Description: Disable Uncore Frequency Scaling to minimize memory latency by forcing the uncore frequency floor to P1 when all cores within the package are in idle -->
          <!-- Possible Values: "Enabled", "Disabled" -->
          <Uncore_Frequency_Scaling>Enabled</Uncore_Frequency_Scaling>
        </CPU_Power_Management_Configuration>
      </Processors>
      <USB_Ports>
